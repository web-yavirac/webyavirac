pipeline { //o tuberias ejecuta escenarios uno por uno 
    agent any
    stages { //escenarios
        stage("Despliegue"){
            agent {
                label 'integracion'
            }
            steps{
                sh 'docker build -f devops/Dockerfile -t webyavirac:latest .'
                sh 'docker stack rm webyavirac-na'
                sh 'docker stack deploy -c devops/stack.yml webyavirac-na'
            }    
        }
       
    }
}